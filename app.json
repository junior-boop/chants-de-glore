{
  "expo": {
    "name": "chants de glore",
    "slug": "chants-de-glore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.junior41.chantsdeglore"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.junior41.chantsdeglore"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "d5a274af-b28b-47ab-812a-d88f133557e8"
      }
    },
    "owner": "junior41",
    "updates": {
      "url": "https://u.expo.dev/d5a274af-b28b-47ab-812a-d88f133557e8"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
